<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="Employee" elementFormDefault="unqualified" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xs:element name="Employee" nillable="true">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="FirstName" type="xs:string" nillable="true" msdata:ReadOnly="false" />
				<xs:element name="LastName" type="xs:string" nillable="true" msdata:ReadOnly="false" />
				<xs:element name="Title" type="xs:string" nillable="true" msdata:ReadOnly="false" />
				<xs:element name="Id" type="xs:string" nillable="true" msdata:ReadOnly="false" />
				<xs:element name="CellPhone" type="xs:string" nillable="true" msdata:ReadOnly="false" />
				<xs:element name="EmployeeAddress" nillable="true">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Address" nillable="true" maxOccurs="unbounded" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Street" type="xs:string" nillable="true" msdata:ReadOnly="false" />
										<xs:element name="City" type="xs:string" nillable="true" msdata:ReadOnly="false" />
										<xs:element name="State" type="xs:string" nillable="true" msdata:ReadOnly="false" />
										<xs:element name="Zip" type="xs:string" nillable="true" msdata:ReadOnly="false" />
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>