<!DOCTYPE HTML>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    
    <title>@ViewBag.Title</title>
    <!--[if lte IE 8]>
        <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
        
        <script>
                top.location.href = "http://localhost:30250/IEError/";
        </script>
             
    <![endif]-->
 
    <!--[if lt IE 8]>
        <link href="~/Content/bootstrap-ie7.css" rel="stylesheet" />
    <![endif]-->

 
    @*@Styles.Render("~/Content/themes/blitzer/css")*@
    
    @Styles.Render("~/Content/themes/jq-ui")
    @Styles.Render("~/Content/site")
    @Styles.Render("~/Content/bootstrapcss")
    @Scripts.Render("~/bundles/jquery")
    <link href="~/Content/ChatRoom.css" rel="stylesheet" />



</head>
<body>

  
    @*top.location.href = "http://windows.microsoft.com/es-ar/internet-explorer/ie-9-worldwide-languages";*@
    <div class="container">
        <div class="row">
            <div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
                <div class="container">

                    <!--Left Align-->
                    <ul class="nav navbar-nav navbar-left">
                        
                        
                    </ul>
                    <div class="col-sm-3 col-md-3 pull-right">
                        <form class="navbar-form" role="search">
                            <div class="input-group">

                                <input type="text" class="form-control" placeholder="Search...">
                                <span class="input-group-addon"><span class="glyphicon glyphicon-search"></span></span>

                            </div>
                        </form>
                    </div>
                    <!--Right Align-->
                    <ul class="nav navbar-nav navbar-right">





                        <li><a href="#">Allus</a></li>
                    </ul>
                </div>
            </div>
            @RenderBody()
        </div>
    </div>
    @*<img id="ajax-loader" class="ajaxloader" src="@Url.Content("~/img/ajax-loader.gif")" style="display:none" />*@
    <div class="footer navbar navbar-default">
        <div class="container">
            <div class="row ">
                <p>&copy; Company Allus Global BPO 2014 </p>
            </div>
        </div>
    </div>


    @Scripts.Render("~/bundles/jquery")


  
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/modernizr")

    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/site")
    @RenderSection("scripts", required: false)
    @Html.Partial("_MessageBox")
</body>
</html>

